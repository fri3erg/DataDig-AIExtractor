{
  "version" : 2,
  "waiters" : {
    "ServerOffline" : {
      "delay" : 30,
      "maxAttempts" : 120,
      "operation" : "DescribeServer",
      "acceptors" : [ {
        "matcher" : "path",
        "argument" : "Server.State",
        "state" : "success",
        "expected" : "OFFLINE"
      ***REMOVED***, {
        "matcher" : "path",
        "argument" : "Server.State",
        "state" : "failure",
        "expected" : "STOP_FAILED"
      ***REMOVED*** ]
    ***REMOVED***,
    "ServerOnline" : {
      "delay" : 30,
      "maxAttempts" : 120,
      "operation" : "DescribeServer",
      "acceptors" : [ {
        "matcher" : "path",
        "argument" : "Server.State",
        "state" : "success",
        "expected" : "ONLINE"
      ***REMOVED***, {
        "matcher" : "path",
        "argument" : "Server.State",
        "state" : "failure",
        "expected" : "START_FAILED"
      ***REMOVED*** ]
    ***REMOVED***
  ***REMOVED***
***REMOVED***