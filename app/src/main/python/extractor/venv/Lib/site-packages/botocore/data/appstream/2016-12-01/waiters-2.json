{
  "version": 2,
  "waiters": {
    "FleetStarted": {
      "delay": 30,
      "maxAttempts": 40,
      "operation": "DescribeFleets",
      "acceptors": [
        {
          "state": "success",
          "matcher": "pathAll",
          "argument": "Fleets[].State",
          "expected": "RUNNING"
        ***REMOVED***,
        {
          "state": "failure",
          "matcher": "pathAny",
          "argument": "Fleets[].State",
          "expected": "STOPPING"
        ***REMOVED***,
        {
          "state": "failure",
          "matcher": "pathAny",
          "argument": "Fleets[].State",
          "expected": "STOPPED"
        ***REMOVED***
      ]
    ***REMOVED***,
    "FleetStopped": {
      "delay": 30,
      "maxAttempts": 40,
      "operation": "DescribeFleets",
      "acceptors": [
        {
          "state": "success",
          "matcher": "pathAll",
          "argument": "Fleets[].State",
          "expected": "STOPPED"
        ***REMOVED***,
        {
          "state": "failure",
          "matcher": "pathAny",
          "argument": "Fleets[].State",
          "expected": "STARTING"
        ***REMOVED***,
        {
          "state": "failure",
          "matcher": "pathAny",
          "argument": "Fleets[].State",
          "expected": "RUNNING"
        ***REMOVED***
      ]
    ***REMOVED***
  ***REMOVED***
***REMOVED***